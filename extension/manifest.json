{
  "manifest_version": 2,
  "version": "1",
  "name": "TimeWise",
  "short_name" : "TW",
  "description": "A fast, simple solution to attack tedious elearning review work.",
  "author": "Pablo Buitrago",
  "offline_enabled": true,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }, 
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "TimeWise Extension",
    "default_popup": "dist/index.html"
  }
}
